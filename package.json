    "intl-parse-accept-language": "^1.0.0",
    "marked": "^4.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-highlight": "^5.0.2",
    "remix-graphql": "^0.2.1",
